    FilterTags: 
    EventId: 
    FixedDurationInSpeech: 0
  - Speeches:
    - LanguageType: 0
      announcerName: ???
      content: '"But I think I saw our old sheriff bringin\u2019 a young lad wearing'
    - LanguageType: 1
      announcerName: ???
