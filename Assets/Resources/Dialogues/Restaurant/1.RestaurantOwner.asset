    FilterTags: 
    EventId: 
    FixedDurationInSpeech: 0
  - Speeches:
    - LanguageType: 0
      announcerName: Cynthia
      content: '"I\u2026 Oh yeah! There was an accident!"'
    - LanguageType: 1
