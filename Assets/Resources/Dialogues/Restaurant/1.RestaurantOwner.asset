    FilterTags: 
    EventId: CynthiaFocus_2
    FixedDurationInSpeech: 0
  - Speeches:
    - LanguageType: 0
      announcerName: Cynthia
      content: A train almost crashed onto our car and we were forced to swerve off
    - LanguageType: 1
